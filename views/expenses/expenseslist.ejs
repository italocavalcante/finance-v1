  <!DOCTYPE html>
  <html>
    <head>
      <!--Import Google Icon Font-->
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <!--Import materialize.css-->
      <link type="text/css" rel="stylesheet" href="/css/materialize.min.css"  media="screen,projection"/>

      <!--Let browser know website is optimized for mobile-->
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    </head>

    <body>
      <nav>
        <div class="nav-wrapper">
          <a href="#" class="brand-logo right">Logo</a>
          <ul id="nav-mobile" class="left hide-on-med-and-down">
            <li><a href="sass.html">Sass</a></li>
            <li><a href="badges.html">Components</a></li>
            <li><a href="collapsible.html">JavaScript</a></li>
          </ul>
        </div>
      </nav>
      <div class="container">
        <table class="striped">
            <thead>
              <tr>
                  <th>id</th>
                  <th>Title</th>
                  <th>Value</th>
                  <th>description</th>
              </tr>
            </thead>
    
            <tbody>
                <%expenses.forEach((expense) => {%>
                    <tr>
                        <td><%= expense.id %></td>
                        <td><%= expense.title %></td>
                        <td><%= expense.value %></td>
                        <td><%= expense.description %></td>
                    </tr>
                <%})%>
            </tbody>
          </table>
          <div align="center">
            <% if(next){ %>
            <button class="waves-effect waves-light btn-small red"><i class="material-icons right red">arrow_forward</i>Next</button>
            <%}else{ %>
            <button class="waves-effect waves-light btn-small red disabled"><i class="material-icons right red">arrow_forward</i>Next</button>
            <%}%> 
          </div>
        </div>
        <!--JavaScript at end of body for optimized loading-->
     <script type="../text/javascript" src="../js/materialize.min.js"></script>
    </body>
  </html>