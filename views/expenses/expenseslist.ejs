  <!DOCTYPE html>
  <html>
    <head>
      <!--Import Google Icon Font-->
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <!--Import materialize.css-->
      <link type="text/css" rel="stylesheet" href="/css/materialize.min.css"  media="screen,projection"/>

      <!--Let browser know website is optimized for mobile-->
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    </head>

    <body>
      <nav>
        <div class="nav-wrapper">
            <a href="#" class="brand-logo right">Logo</a>
          <ul id="nav-mobile" class="left hide-on-med-and-down">
            <li><a href="sass.html">Sass</a></li>
            <li><a href="badges.html">Components</a></li>
            <li><a href="collapsible.html">JavaScript</a></li>
            <li><a href="/exit">Exit</a></li>
          </ul>
        </div>
      </nav>
      <div class="container">
        <table class="striped">
            <thead>
              <tr>
                  <th class="center">id</th>
                  <th>Title</th>
                  <th>Value</th>
                  <th>description</th>
                  <th>Expense Date</th>
              </tr>
            </thead>
    
            <tbody>
                <%expenses.forEach((expense) => {%>
                    <tr>
                        <td class="center"><a href="/expenses/editar/<%= expense.id %>"   class="waves-effect waves-light btn-small red center" ><i class="material-icons center red">edit</i></a></td>
                        <td><%= expense.title %></td>
                        <td><%= expense.value %></td>
                        <td><%= expense.description %></td>
                        <td><%= expense.dateExpense.toLocaleString() %></td>
                    </tr>
                <%})%>
            </tbody>
          </table>
          <div class="row m6">
            <% if(page == 2 || page>2){ %>
            <div class="col s4"><a href="/expenses/page/<%=page - 1%>" class="waves-effect waves-light btn-small red" ><i class="material-icons left red">arrow_back</i>Prev</a></div>
            <%}else{%>
            <div class="col s4"></div>
            <%}%>
            <% if (next == true){ %>
            <a href="/expenses/page/<%=page + 1%>"   class="waves-effect waves-light btn-small red right" ><i class="material-icons right red">arrow_forward</i>Next</a>
            <div class="col s4"><div class="col s4"></div><a href="/expenses/addexpense"   class="waves-effect waves-light btn-small red center" ><i class="material-icons left red">add</i>Add expense</a> </div>
          </div>
            <%}%>
        </div>
        <div class="row">

        </div>
        <!--Footer configuration below-->
        <footer class="page-footer red">
          <div class="container">
            <div class="row">
              <div class="col l6 s12">
                <h5 class="white-text">Footer Content</h5>
                <p class="grey-text text-lighten-4">You can use rows and columns here to organize your footer content.</p>
              </div>
              <div class="col l4 offset-l2 s12">
                <h5 class="white-text">Links</h5>
                <ul>
                  <li><a class="grey-text text-lighten-3" href="#!">Link 1</a></li>
                  <li><a class="grey-text text-lighten-3" href="#!">Link 2</a></li>
                  <li><a class="grey-text text-lighten-3" href="#!">Link 3</a></li>
                  <li><a class="grey-text text-lighten-3" href="#!">Link 4</a></li>
                </ul>
              </div>
            </div>
          </div>
          <div class="footer-copyright">
            <div class="container">
            Â© 2014 Copyright Text
            <a class="grey-text text-lighten-4 right" href="#!">More Links</a>
            </div>
          </div>
        </footer>
        <!--JavaScript at end of body for optimized loading-->
     <script type="../text/javascript" src="../js/materialize.min.js"></script>
    </body>
  </html>